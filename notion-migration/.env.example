# Notion API credentials and configuration
# Copy this file to .env and fill in your values

# Your Notion integration token (starts with 'secret_' or 'ntn_')
# Create one at https://www.notion.so/my-integrations
NOTION_API_KEY=your_notion_api_key_here

# ID of your Technical Notes page in Notion
# This is the long string in the page URL after the workspace name
# Example: For https://www.notion.so/workspace/d5e4e5143d2c4a6fa8ca3ab2f162c22c
# The SOURCE_PAGE_ID would be: d5e4e5143d2c4a6fa8ca3ab2f162c22c
SOURCE_PAGE_ID=your_source_page_id_here

# ID of the target database to populate with your notes
# If you already have a database, get the ID from its URL
# If not, leave this empty and the script can create one for you
NOTION_DATABASE_ID=your_database_id_here

# Optional: Name of the database to create if it doesn't exist
# Only used if the database needs to be created
TARGET_DATABASE_NAME=your_database_name_here

# AI API Keys for Database Update Tool
# Your DeepSeek API key for generating summaries and reading times
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Your Gemini API key for generating images with Imagen 3
GEMINI_API_KEY=your_gemini_api_key_here

# Your DashScope API key for generating images with Alibaba DashScope
# Get one from https://dashscope.aliyun.com/
DASHSCOPE_API_KEY=your_dashscope_api_key_here

# Cloudflare R2 Configuration
# For image migration from Notion to R2 storage
# Your Cloudflare account ID (found in the R2 dashboard URL)
R2_ACCOUNT_ID=your_r2_account_id_here

# R2 access credentials (create these in the R2 dashboard)
R2_ACCESS_KEY_ID=your_r2_access_key_id_here
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key_here

# R2 bucket name where images will be stored
R2_BUCKET_NAME=your_bucket_name_here

# Public URL prefix for accessing images (from R2 dashboard)
R2_PUBLIC_URL=https://your-bucket-public-url.r2.dev

# Example values:
# NOTION_API_KEY=ntn_123456789abcdef...
# SOURCE_PAGE_ID=d5e4e5143d2c4a6fa8ca3ab2f162c22c
